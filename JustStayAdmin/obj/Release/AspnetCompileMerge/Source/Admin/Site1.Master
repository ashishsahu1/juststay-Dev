       <script src="js/bootstrap.min.js"></script>
        <script src="js/materialize.min.js"></script>
        <script src="js/custom.js"></script>
        <script>
            function ShowChangePwdPopup() {
                $('.modal').modal();
                $('#txtcurrentpass').val('');
                $('#txtpassnew').val('');
                $('#txtpassconfirm').val('');
                if ($('#hdnpassid').val() == '0') {
                    $('#lblchange').text('');
                }
                $('#changepwd').modal('open');
                $('.trigger-modal').modal();
            }
        </script>
