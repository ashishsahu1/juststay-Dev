        <div id="changepwd" class="modal fade" role="dialog">
            <div class="modal-dialog">
                <div class="modal-header">
                    <h4 class="modal-title">Change Password</h4>
                </div>
                <div class="modal-body">
                    <div class="col-md-12">
                        <p>
                            <asp:Label ID="lblchange" runat="server" Style="font-size: 13px;"></asp:Label>
                        </p>
                        <div class="form-group">
                            <label>Current Password</label>
                            <asp:TextBox ID="txtcurrentpass" runat="server" MaxLength="50" CssClass="form-control" placeholder="Current Password"></asp:TextBox>
                            <asp:RequiredFieldValidator ID="rfvpass" runat="server"
                                Display="Dynamic" SetFocusOnError="true" ForeColor="Red"
                                ControlToValidate="txtcurrentpass" ValidationGroup="passchg">
                                </asp:RequiredFieldValidator>
                        </div>
                        <div class="form-group">
                            <label>New Password</label>
                            <asp:TextBox ID="txtpassnew" runat="server" MaxLength="50" CssClass="form-control" placeholder="New Password"></asp:TextBox>
                            <asp:RequiredFieldValidator ID="rfvnewpass" runat="server"
                                Display="Dynamic" SetFocusOnError="true" ForeColor="Red"
                                ControlToValidate="txtpassnew" ValidationGroup="passchg">
                                </asp:RequiredFieldValidator>
                        </div>
                        <div class="form-group">
                            <label>Confirm Password</label>
                            <asp:TextBox ID="txtpassconfirm" runat="server" MaxLength="50" CssClass="form-control" placeholder="Confirm Password"></asp:TextBox>
                            <asp:RequiredFieldValidator ID="rfvpassconfirm" runat="server"
                                Display="Dynamic" SetFocusOnError="true" ForeColor="Red"
                                ControlToValidate="txtpassconfirm" ValidationGroup="passchg">
                                </asp:RequiredFieldValidator>
                        </div>
                    </div>
                </div>
                <div class="modal-footer" style="border-top: none;">
                    <asp:LinkButton ID="btnclose" runat="server" class="btn btn-primary" CausesValidation="false" PostBackUrl="~/Admin/dashboard.aspx">Close</asp:LinkButton>
                    <asp:LinkButton ID="btnChangePwd" runat="server" class="btn btn-primary" CausesValidation="true" ValidationGroup="passchg" OnClick="btnChangePwd_Click">Change</asp:LinkButton>
                </div>
            </div>
        </div>
        
        <asp:HiddenField ID="hdnpassid" runat="server" Value="0" />
